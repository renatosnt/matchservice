{
    "collectCoverageFrom": [
        "**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
        "node_modules",
        "backend/domain/ports/*",
        "backend/index.ts",
        "application/express/express.ts",
        "application/express/swagger.ts",
        "application/express/routes/handlers/tests/mocks.ts",
        "<rootDir>/intrastructure/prisma-client.ts"
    ],
    "setupFilesAfterEnv": ["<rootDir>/intrastructure/tests/prisma-mock.ts"]
}